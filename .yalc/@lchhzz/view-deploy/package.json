{
  "name": "@lchhzz/view-deploy",
  "version": "2.0.1",
  "description": "一个可自定义的 SSH 部署工具，支持前端项目部署，自动转换 Windows/Unix 命令",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "view-deploy": "dist/cli.js",
    "cl-vd": "dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc",
    "cli": "node dist/cli.js",
    "postinstall": "node dist/scripts/setup.js",
    "deploy": "npm run dev -- deploy",
    "deploy:init": "npm run dev -- init",
    "deploy:config": "npm run dev config --",
    "deploy:test": "npm run dev test --",
    "deploy:reset": "npm run dev reset --",
    "pu": "npm publish --registry=https://registry.npmjs.org/ --access=public"
  },
  "keywords": [
    "deploy",
    "ssh",
    "automation",
    "cli"
  ],
  "author": "cl",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.5.0",
    "inquirer": "^8.2.5",
    "ssh2": "^1.15.0"
  },
  "engines": {
    "node": ">=18.20.4"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/luochuanccc/cl-deploy"
  },
  "yalcSig": "882fef2e03bcadb8a299e2b7825a8ed4"
}
